{
    "of-port": 1001,
    "manage-port": 1002,

    "server-ip": "192.168.0.100",
    "client-ips": [
        "192.168.0.101",
        "192.168.0.102",
        "192.168.0.103",
        "192.168.0.104",
        "192.168.0.105"
    ],

    "server-do-manage-clients": true,

    "fullscreen-server": false,
    "fullscreen-client": true,
    "server-view-mode": "channels",
    "client-view-mode": "status",
    "font-path": "font/klima-medium-web.ttf",

    "enable-log-to-file": false,
    "test-content-mode": false,
    "number-of-video-channels": 3,
    "min-decoy-duration-secs": 7.5,

    "machine-names": {
        "server": "machine-name"
    },

    "server-volumes": {
        "master": 1,
        "breathing": 0.85,
        "video": 0.75,
        "ecg": 1,
        "static": 0.45
    },

    "ecg-beep": {
        "highest-bpm": 46,
        "lowest-bpm": 30,
        "bpm-period-secs": 180,
        "begin-at": 0.34,
        "end-at": 0.44
    },

    "media-sequencing": {
        "shortest-delay": 500,
        "longest-delay": 8000
    },

    "media-files": {
        "live-path": "../path-to/live/",
        "preview-path": "../path-to/preview/",
        "test-content-live-path": "../path-to/test/live/",
        "test-content-preview-path": "../path-to/test/preview/",
        "breathing-sound-path": "../path-to/breathing.wav",
        "live-dir": "/live/",
        "preview-dir": "/preview/",
        "video-file-extension": "mp4"
    }
}